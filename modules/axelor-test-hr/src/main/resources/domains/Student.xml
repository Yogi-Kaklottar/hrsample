<?xml version="1.0" encoding="UTF-8"?>
<domain-models
  xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">
   <module name="hr" package="com.axelor.hr.db" />
  <entity name="Student" cacheable="true">
  <string name="name"/>
  <many-to-one name="studentimage" title="picture" ref="com.axelor.meta.db.MetaFile"/>
 <integer name="marks1" ></integer>
 <integer name="marks2" ></integer>
 <integer name="marks3" ></integer>
 <integer name="total"  ></integer>
 <string name="classname"/>
 <integer name="greadselect" selection="selection-student-gread"/>
 <integer name="progress" selection="selection-student-column"/>
  <decimal name="persantage" precision="5" scale="2"></decimal>
 <string name="grade"></string>
  </entity>
</domain-models>
